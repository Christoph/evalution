<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Presentation="clr-namespace:Presentation"
    xmlns:View="clr-namespace:Presentation.View">

    <!-- 
    This template applies an AllCustomersView to an instance 
    of the AllCustomersViewModel class shown in the main window.
    -->
    <DataTemplate DataType="{x:Type Presentation:AllBinaryQuestionsViewModel}">
        <View:AllBinaryQuestionsView/>
    </DataTemplate>

    <!-- 
    This template applies a CustomerView to an instance  
    of the CustomerViewModel class shown in the main window.
    -->
    <DataTemplate x:Key="BinaryQuestion" DataType="{x:Type Presentation:BinaryQuestionViewModel}">
        <View:BinaryQuestionView/>
    </DataTemplate>

</ResourceDictionary>